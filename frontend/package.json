{
  "name": "music-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup:env": "cp .env.sample .env",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix src",
